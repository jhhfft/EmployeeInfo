<body>
  <form class="ui form container employee" autocomplete="off" onkeydown="if(event.keyCode==13)return false;">
    <table class="ui table  selectable celled structured fixed unstackable" style="margin: 0px;">
      <tbody>
        <tr>
          <td class="two wide cell-name">姓名<i style="color:red; font-size: 16px;">*</i></td>
          <td class="four wide cell-name">
            <div class="ui input transparent">
              <input type="text" name="name" id="name" value=<%=name %>>
            </div>
          </td>
          <td class="two wide cell-sex">性别<i style="color:red; font-size: 16px;">*</i></td>
          <td class="four wide cell-sex">
            <div class="inline field">
              <span></span>
              <% if (sex == '男') { %>
              <input type="radio" name="sex" value="男" checked>
              <% } else { %>
              <input type="radio" name="sex" value="男">
              <% } %>
              <label style="display:inline-block;margin-right:20px;">男</label>
              <% if (sex == '女') { %>
              <input type="radio" name="sex" value="女" checked>
              <% }else { %>
              <input type="radio" name="sex" value="女">
              <% } %>
              <label>女</label>
            </div>
          </td>
          <% if (contract == 0) { %>
            <td class="two wide cell-protrait" rowspan="5" id="portrait" style="background: lightgoldenrodyellow">
          <% } else { %>
            <td class="two wide cell-protrait" rowspan="5" id="portrait">
          <% } %>
          
            <input type="file" name="headportrait" id="headportrait" name="portrait" />
            <label for="headportrait">上传图片</label>
            <p></p>
            <img alt="" src="<%= '/' + portrait%>">
          </td>
        </tr>
        <tr>
          <td class="cell-nation">民族<i style="color:red; font-size: 16px;">*</i></td>
          <td class="four wide cell-nation">
            <div class="ui input transparent">
              <input type="text" name="nation" id="nation" value=<%=nation %>>
            </div>
          </td>
          <td class="cell-birthday">出生日期<i style="color:red; font-size: 16px;">*</i></td>
          <td class="four wide cell-birthday" style="overflow:visible">
            <div class="ui calendar date-picker">
              <div class="ui input left icon  input transparent">
                <i class="calendar icon"></i>
                <input type="text" name="birthday" value=<%=birthday %>>
              </div>
            </div>
          </td>
        </tr>
        <tr>
